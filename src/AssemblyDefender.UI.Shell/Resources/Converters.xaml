<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:wincor="clr-namespace:AssemblyDefender.UI;assembly=AssemblyDefender.UI"
    xmlns:shell="clr-namespace:AssemblyDefender.UI.Shell">

	<wincor:IsNullConverter x:Key="isNullConverter" />
	<wincor:BoolConverter x:Key="boolConverter" TrueValue="True" FalseValue="False" />
	<wincor:BoolConverter x:Key="invertBoolConverter" TrueValue="False" FalseValue="True" />
	<wincor:VisibilityConverter x:Key="visibleCollapsedConverter" TrueValue="Visible" FalseValue="Collapsed" />
	<wincor:VisibilityConverter x:Key="collapsedVisibleConverter" TrueValue="Collapsed" FalseValue="Visible" />
	<wincor:VisibilityConverter x:Key="visibleHiddenConverter" TrueValue="Visible" FalseValue="Hidden" />
	<wincor:VisibilityConverter x:Key="hiddenVisibleConverter" TrueValue="Hidden" FalseValue="Visible" />
	<wincor:MultiplyConverter x:Key="multiplyConverter" />
	<wincor:SizeToStringConverter x:Key="sizeToStringConverter" />
	<wincor:EnumToBooleanConverter x:Key="enumToBoolConverter" />
	<wincor:DateTimeToStringConverter x:Key="dateTimeToStringConverter" />
	<wincor:ImageConverter x:Key="image16Converter" Height="16" Width="16" />
	<shell:ImageTypeToSourceConverter x:Key="imageTypeToSourceConverter" />
	<shell:ImageTypeToImageConverter x:Key="imageTypeToImage16Converter" Height="16" Width="16" />

</ResourceDictionary>
